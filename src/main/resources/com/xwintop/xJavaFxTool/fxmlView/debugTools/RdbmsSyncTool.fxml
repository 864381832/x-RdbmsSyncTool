                                                <Tooltip text="单位为秒" />
                                             </tooltip>
                                          </Spinner>
                                          <Label layoutX="101.0" layoutY="4.0" text="次数" />
                                          <Spinner fx:id="repeatCountSpinner" editable="true" layoutX="129.0" prefHeight="23.0" prefWidth="64.0">
